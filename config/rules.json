{
  "employees": {
    "Name": ["not_null"],
    "Gender": ["not_null"],
    "Age":["ranges"],
    "City": ["not_null"],
    "Department": ["not_null"],
    "Salary": ["ranges"],
    "Email": ["not_null", "regex"]
  },
  "rules_config": {
    "regex_patterns": {
      "Email": "[^@]+@[^@]+\\.[^@]+"
    },
    "ranges": {
      "Age": [14, 100],
      "Salary": [20000, 400000]
    }
  }
}