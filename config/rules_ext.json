{
  "employees": {
    "First Name": ["not_null"],
    "Last Name": ["not_null"],
    "Email": ["not_null", "regex"],
    "Phone": ["not_null", "regex"],
    "Gender": ["not_null", "allowed_values"],
    "Age": ["range"],
    "Job Title": ["not_null"],
    "Years Of Experience": ["range"],
    "Salary": ["range"],
    "Department": ["not_null"]
  },
  "rules_config": {
    "regex_patterns": {
      "Email": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$",
      "Phone": "^[0-9\\-\\+]{10,15}$"
    },
    "ranges": {
      "Age": [14, 100],
      "Years Of Experience": [0, 80],
      "Salary": [20000, 400000]
    },
    "allowed_values": {
      "Gender": ["Male", "Female", "Other", "Prefer Not to Say"]
    }
  }
}
