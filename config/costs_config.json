    ],
    "CUST_ID": [
      "not_null"
    ],
    "TIME_ID": [
      "not_null"
    ],
    "CHANNEL_ID": [
      "not_null"
    ],
    "PROMO_ID": [
      "not_null"
    ],
    "QUANTITY_SOLD": [
      "not_null"
    ],
    "AMOUNT_SOLD": [
      "not_null"
    ]
